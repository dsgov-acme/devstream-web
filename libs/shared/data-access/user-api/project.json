{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "name": "shared-data-access-user-api",
  "prefix": "dsg",
  "projectType": "library",
  "sourceRoot": "libs/shared/data-access/user-api/src",
  "tags": ["type:data-access", "scope:shared"],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/shared/data-access/user-api/**/*.ts", "libs/shared/data-access/user-api/**/*.html"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      },
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/shared/data-access/user-api/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"]
    }
  }
}
